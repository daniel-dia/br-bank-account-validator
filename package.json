{
  "name": "br-bank-account-validator",
  "version": "0.0.2",
  "description": "rm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-dia/br-bank-account-validator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniel-dia/br-bank-account-validator.git/issues"
  },
  "homepage": "https://github.com/daniel-dia/br-bank-account-validator.git",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.ts",
      "!src/main.ts",
      "!dist/**",
      "!node_modules/**"
    ],
    "testEnvironment": "node"
  }
}